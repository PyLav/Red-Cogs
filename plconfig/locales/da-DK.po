#: plconfig/cog.py:29
#, docstring
msgid "Configure PyLav library settings."
msgstr "Indstil indstillinger for PyLav bibliotek."

#: plconfig/cog.py:42
#, docstring
msgid "Change global settings for PyLav"
msgstr "Skift globale indstillinger for PyLav"

#: plconfig/cog.py:46
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Vis versionen af Cog og det er PyLav afhængigheder."

#: plconfig/cog.py:59
msgid "Library/Cog"
msgstr "Bibliotek/Tåge"

#: plconfig/cog.py:59
msgid "Version"
msgstr "Version"

#: plconfig/cog.py:67
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Indstil mappen for PyLavs konfigurationsfiler\n\n"
"        Ændringer vil blive anvendt efter genstart af bot.\n"
"        "

#: plconfig/cog.py:79 plconfig/cog.py:133
msgid "{folder} is not a folder."
msgstr "{folder} er ikke en mappe."

#: plconfig/cog.py:93 plconfig/cog.py:147
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} eksisterer ikke, kør kommandoen igen med det argument der er sat til 1 for automatisk at oprette denne mappe."

#: plconfig/cog.py:111
msgid "PyLav's config folder has been set to {folder}."
msgstr "PyLavs konfigurationsmappe er indstillet til {folder}."

#: plconfig/cog.py:121
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Indstil den lokale spormappe for PyLav.\n\n"
"        Ændringer vil blive anvendt efter genstart af bot.\n"
"        "

#: plconfig/cog.py:168
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "PyLavs lokale spormappe er blevet indstillet til {folder}."

#: plconfig/cog.py:178
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "

#: plconfig/cog.py:195
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} eksisterer ikke, kør kommandoen igen med java argumentet sat til den korrekte sti."

#: plconfig/cog.py:211
msgid "{java} is not an executable file."
msgstr "{java} er ikke en eksekverbar fil."

#: plconfig/cog.py:223
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} er ikke en eksekverbar, kør kommandoen igen med java argumentet sat til den korrekte sti."

#: plconfig/cog.py:241
msgid "PyLav's java executable has been set to {java}."
msgstr "PyLavs java eksekverbare er blevet indstillet til {java}."

#: plconfig/cog.py:251
#, docstring
msgid "Change the managed node configuration."
msgstr "Ændr den administrerede indholdselementkonfiguration."

#: plconfig/cog.py:255
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Slå den administrerede node til/fra.\n\n"
"        Ændringer vil blive anvendt efter genstart af bot.\n"
"        "

#: plconfig/cog.py:270
msgid "PyLav's managed node has been enabled."
msgstr "PyLavs styrede indholdselement er blevet aktiveret."

#: plconfig/cog.py:278
msgid "PyLav's managed node has been disabled."
msgstr "PyLavs styrede indholdselement er blevet deaktiveret."

#: plconfig/cog.py:286
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Slå den administrerede node auto opdateringer til/fra.\n\n"
"        Ændringer vil blive anvendt efter genstart af bot.\n"
"        "

#: plconfig/cog.py:301
msgid "PyLav's managed node auto updates have been enabled."
msgstr "PyLavs administrerede node auto-opdateringer er blevet aktiveret."

#: plconfig/cog.py:309
msgid "PyLav's managed node auto updates have been disabled."
msgstr "PyLavs administrerede node auto-opdateringer er blevet deaktiveret."

#: plconfig/cog.py:317
#, docstring
msgid "Toggle the managed external node on/off."
msgstr "Slå den administrerede eksterne knude til/fra."

#: plconfig/cog.py:335
msgid "PyLav's managed external node has been enabled."
msgstr "PyLavs administrerede eksterne node er blevet aktiveret."

#: plconfig/cog.py:343
msgid "PyLav's managed external node has been disabled."
msgstr "PyLavs administrerede eksterne node er blevet deaktiveret."

