#: plconfig/cog.py:29
#, docstring
msgid "Configure PyLav library settings."
msgstr "Configurer les paramètres de la bibliothèque PyLav."

#: plconfig/cog.py:42
#, docstring
msgid "Change global settings for PyLav"
msgstr "Modifier les paramètres globaux pour PyLav"

#: plconfig/cog.py:46
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Affiche la version du Cog et ses dépendances PyLav."

#: plconfig/cog.py:59
msgid "Library/Cog"
msgstr "Bibliothèque/Cog"

#: plconfig/cog.py:59
msgid "Version"
msgstr "Version"

#: plconfig/cog.py:67
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Définit le dossier des fichiers de configuration de PyLav\n\n"
"        Les modifications seront appliquées après le redémarrage du bot.\n"
"        "

#: plconfig/cog.py:79 plconfig/cog.py:133
msgid "{folder} is not a folder."
msgstr "{folder} n'est pas un dossier."

#: plconfig/cog.py:93 plconfig/cog.py:147
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} n'existe pas, exécutez la commande à nouveau avec l'argument create à 1 pour créer automatiquement ce dossier."

#: plconfig/cog.py:111
msgid "PyLav's config folder has been set to {folder}."
msgstr "Le dossier de configuration de PyLava a été réglé sur {folder}."

#: plconfig/cog.py:121
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Définit le dossier de pistes locales pour PyLav.\n\n"
"        Les modifications seront appliquées après le redémarrage du bot.\n"
"        "

#: plconfig/cog.py:168
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "Le dossier de pistes locales de PyLava a été réglé sur {folder}."

#: plconfig/cog.py:178
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Définit l'exécutable java pour PyLav.\n\n"
"        La valeur par défaut est \"java\"\n"
"        Les changements seront appliqués après le redémarrage du bot.\n"
"        "

#: plconfig/cog.py:195
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} n'existe pas, exécutez la commande à nouveau avec l'argument java défini au bon chemin."

#: plconfig/cog.py:211
msgid "{java} is not an executable file."
msgstr "{java} n'est pas un fichier exécutable."

#: plconfig/cog.py:223
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} n'est pas un exécutable, exécutez à nouveau la commande avec l'argument java défini au bon chemin."

#: plconfig/cog.py:241
msgid "PyLav's java executable has been set to {java}."
msgstr "L'exécutable java de PyLave a été réglé sur {java}."

#: plconfig/cog.py:251
#, docstring
msgid "Change the managed node configuration."
msgstr "Modifier la configuration du noeud géré."

#: plconfig/cog.py:255
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Activer/désactiver le noeud géré.\n\n"
"        Les modifications seront appliquées après le redémarrage du bot.\n"
"        "

#: plconfig/cog.py:270
msgid "PyLav's managed node has been enabled."
msgstr "Le noeud géré par Pylave a été activé."

#: plconfig/cog.py:278
msgid "PyLav's managed node has been disabled."
msgstr "Le noeud géré par Pylave a été désactivé."

#: plconfig/cog.py:286
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Activer/désactiver les mises à jour automatiques des nœuds gérés.\n\n"
"        Les modifications seront appliquées après le redémarrage du bot.\n"
"        "

#: plconfig/cog.py:301
msgid "PyLav's managed node auto updates have been enabled."
msgstr "Les mises à jour automatiques de nœuds gérées par PyLav."

#: plconfig/cog.py:309
msgid "PyLav's managed node auto updates have been disabled."
msgstr "Les mises à jour automatiques de nœuds gérées par PyLav, ont été désactivées."

#: plconfig/cog.py:317
#, docstring
msgid "Toggle the managed external node on/off."
msgstr "Activer/désactiver le noeud externe géré."

#: plconfig/cog.py:335
msgid "PyLav's managed external node has been enabled."
msgstr "Le noeud externe géré par PyLave a été activé."

#: plconfig/cog.py:343
msgid "PyLav's managed external node has been disabled."
msgstr "Le noeud externe géré par Pylave a été désactivé."

