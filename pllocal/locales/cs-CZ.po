#: pllocal/cog.py:29
#, docstring
msgid "Play local files and folders from the owner configured location."
msgstr "Přehrávání lokálních souborů a složek od vlastníka nakonfigurovaného umístění."

#: pllocal/cog.py:54
#, docstring
msgid "Configure cog settings."
msgstr "Konfigurovat nastavení cog."

#: pllocal/cog.py:58
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Ukaž verzi Cog a jsou to PyLav závislosti."

#: pllocal/cog.py:71
msgid "Library/Cog"
msgstr "Knihovna/Cog"

#: pllocal/cog.py:71
msgid "Version"
msgstr "Verze"

#: pllocal/cog.py:85
#, docstring
msgid "Play a local file or folder, supports partial searching."
msgstr "Přehrávání místního souboru nebo složky, podporuje částečné vyhledávání."

#: pllocal/cog.py:101
msgid "You must be in a voice channel to allow me to connect."
msgstr "Musíš být v hlasovém kanálu, abych se mohl připojit."

#: pllocal/cog.py:114
msgid "I don't have permission to connect or speak in {channel}."
msgstr "Nemám oprávnění připojit se nebo mluvit v {channel}."

#: pllocal/cog.py:139
msgid "{track_count} tracks enqueued."
msgstr "{track_count} skladeb ve frontě."

#: pllocal/cog.py:147
msgid "{track} enqueued."
msgstr "{track} do fronty."

#: pllocal/cog.py:157
msgid "No tracks were found for your query."
msgstr "Pro váš dotaz nebyly nalezeny žádné stopy."

